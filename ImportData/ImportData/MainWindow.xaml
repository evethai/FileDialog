<Window x:Class="ImportData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportData"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid Name="dgData" AutoGenerateColumns="True" Margin="0,197,0,-10"/>
        <ComboBox IsEnabled="False" Name="cbYear" SelectionChanged="cbYear_SelectionChanged" Width="64" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="52,28,0,0">
        </ComboBox>
        <TextBox Name="txtName" HorizontalAlignment="Left" Margin="197,26,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="294" Height="24"/>
        <Button Content="Browser" HorizontalAlignment="Left" Margin="531,23,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button x:Name="btnImport" IsEnabled="False" Content="Import" HorizontalAlignment="Left" Margin="592,23,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Button x:Name="btnClear" IsEnabled="False" Content="Clear" HorizontalAlignment="Left" Margin="648,23,0,0" VerticalAlignment="Top" Click="Button_Click_2"/>

    </Grid>
</Window>
